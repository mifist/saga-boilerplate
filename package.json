  "license": "ISC",
  "scripts": {
    "generate-resources": "node ./config/scripts/resources.js",
    "prebuild": "rimraf ./build",
    "zip-bundle": "node ./config/scripts/bundle-archive.js",
    "zip-bundle-dev": "cross-env NODE_ENV=production BASE_ENV=development node ./config/scripts/bundle-archive.js",
    "zip-bundle-staging": "cross-env NODE_ENV=production BASE_ENV=staging node ./config/scripts/bundle-archive.js",
    "zip-bundle-prod": "cross-env NODE_ENV=production BASE_ENV=production node ./config/scripts/bundle-archive.js",
    "start:pre": "cross-env NODE_ENV=development BASE_ENV=local nodemon ./config/server/server.js",
    "start": "npm run start:pre",
    "build:production": "cross-env NODE_ENV=production BASE_ENV=production webpack --config ./config/webpack/webpack.prod.js",
    "build:staging": "cross-env NODE_ENV=production BASE_ENV=staging webpack --config ./config/webpack/webpack.prod.js",
    "build:development": "cross-env NODE_ENV=development BASE_ENV=development webpack --config ./config/webpack/webpack.dev.js",
    "build:local": "cross-env NODE_ENV=development BASE_ENV=local webpack --config ./config/webpack/webpack.dev.js"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 11"
  ],
  "resolutions": {
    "babel-core": "7.0.0-beta.0"
  },
